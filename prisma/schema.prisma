generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Score {
  id        String   @id @default(cuid())
  name      String
  wpm       Int
  accuracy  Float
  rawWpm    Int
  createdAt DateTime @default(now())
  
  @@index([wpm])
}